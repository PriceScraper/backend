{
  "containers": {
    "pricescraperbackend": {
      "image": "",
      "ports": {
        "8080": "HTTP"
      }
    }
  },
  "publicEndpoint": {
    "containerName": "pricescraperbackend",
    "containerPort": 8080,
    "healthCheck": {
      "healthyThreshold": 2,
      "unhealthyThreshold": 2,
      "timeoutSeconds": 8,
      "intervalSeconds": 45,
      "path": "/healthcheck",
      "successCodes": "200-499"
    }
  }
}